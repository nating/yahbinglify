{
  "name": "Yahbinglify",
  "version": "1",
  "description": "Replaces occurences of Yahoo, Bing and Google related text and images with Yahbingle related text and images on webpages you visit.",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "images/yahbingle-icon16.png",
    "32": "images/yahbingle-icon32.png",
    "48": "images/yahbingle-icon48.png",
    "128": "images/yahbingle-icon128.png"
  },
  "web_accessible_resources": ["yahbingle.png"],
  "manifest_version": 2
}
